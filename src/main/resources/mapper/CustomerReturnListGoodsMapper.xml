<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.atguigu.jxc.dao.CustomerReturnListGoodsDao">

<!--        Integer deleteBycustomerReturnListGoodsId(Integer customerReturnListId);-->

    <delete id="deleteBycustomerReturnListGoodsId"  >
        DELETE FROM t_customer_return_list_goods
        WHERE customer_return_list_id = #{customerReturnListId}
    </delete>

<!--    <delete id="deleteGoods" parameterType="java.lang.Integer">-->
<!--        DELETE FROM-->
<!--        t_goods-->
<!--        WHERE t_goods.goods_id = #{goodsId}-->
<!--    </delete>-->



    <!--    List<CustomerReturnListGoods> queryCustomerReturnListGoods(Integer customerReturnListId);-->

    <resultMap id="queryCustomerReturnListGoodsResultMap" type="com.atguigu.jxc.entity.CustomerReturnListGoods">
        <result column="customer_return_list_goods_id" property="customerReturnListGoodsId"/>
        <result column="goods_id" property="goodsId"/>
        <result column="goods_code" property="goodsCode"/>
        <result column="goods_name" property="goodsName"/>
        <result column="goods_model" property="goodsModel"/>
        <result column="goods_num" property="goodsNum"/>
        <result column="goods_unit" property="goodsUnit"/>
        <result column="price" property="price"/>
        <result column="total" property="total"/>
        <result column="customer_return_list_id" property="customerReturnListId"/>
        <result column="goods_type_id" property="goodsTypeId"/>
    </resultMap>

    <select id="queryCustomerReturnListGoods" resultMap="queryCustomerReturnListGoodsResultMap">
        SELECT *
        FROM t_customer_return_list_goods t1
        WHERE t1.customer_return_list_id = #{customerReturnListId}
    </select>



</mapper>
